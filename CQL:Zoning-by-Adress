# Python code for this

# Define the CQL table creation statement
cql_create_table = '''
CREATE TABLE zoning_by_address (
    objectid int PRIMARY KEY,
    full_street_name text,
    zoning_ztype text,
    base_zone text,
    base_zone_category text,
    parent_place_id int,
    place_id int,
    segment_id int
);
'''

# Generate CQL insert statements for the first few rows
insert_statements = []
for index, row in df.head(5).iterrows():
    insert_statements.append(f"INSERT INTO zoning_by_address (objectid, full_street_name, zoning_ztype, base_zone, base_zone_category, parent_place_id, place_id, segment_id) VALUES ({row['OBJECTID']}, '{row['FULL_STREET_NAME']}', '{row['ZONING_ZTYPE']}', '{row['BASE_ZONE']}', '{row['BASE_ZONE_CATEGORY']}', {row['PARENT_PLACE_ID']}, {row['PLACE_ID']}, {row['SEGMENT_ID']});")

# Print the CQL table creation statement and the insert statements
print(cql_create_table)
for statement in insert_statements:
    print(statement)

print("CQL statements generated successfully.")
